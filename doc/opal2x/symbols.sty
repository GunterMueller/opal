%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%%
%% The special symbols
%%
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%                                                    %
{\catcode`\_=12%                            % other character
 \catcode`\^=12%                            % other character
\gdef\SetAllSymbols{%                                 %
%
% test, whether amssymb.sty is loaded
%
  \newif\if@AmsSymb
  \expandafter\ifx\csname lozenge\endcsname\relax\@AmsSymbfalse%
  \else\@AmsSymbtrue\fi
%Relations
  \SetSymbol{=<}{\leq}
  \SetSymbol{>=}{\geq}
  \SetSymbol{<<}{\ll}
  \SetSymbol{>>}{\gg}
  \SetAmsSymbol{<.}{\lessdot}{\mathrel{<\hskip-0.6em\cdot}}
  \SetAmsSymbol{>.}{\gtrdot}{\mathrel{\cdot\hskip-0.6em>}}
  \SetAmsSymbol{.>}{\gtrdot}{\mathrel{\cdot\hskip-0.6em>}}
  \SetSymbol{=.}{\doteq}
  \SetAmsSymbol{/<}{\nless}{\not<}
  \SetAmsSymbol{/>}{\ngtr}{\not>}
  \SetAmsSymbol{/=<}{\nleq}{\not\leq}
  \SetAmsSymbol{/>=}{\ngeq}{\not\geq}
  \SetSymbol{/=}{\neq}
  \SetSymbol{===}{\equiv}
  \SetSymbol{=/=}{\not\equiv}
% Arrows
  \SetSymbol{=>}{\Rightarrow}
  \SetSymbol{<=}{\Leftarrow}
  \SetSymbol{<=>}{\Leftrightarrow}
  \SetSymbol{==>}{\Longrightarrow}
  \SetSymbol{<==}{\Longleftarrow}
  \SetSymbol{<==>}{\Longleftrightarrow}
  \SetSymbol{->}{\rightarrow}
  \SetSymbol{<-}{\leftarrow}
  \SetSymbol{<->}{\leftrightarrow}
  \SetSymbol{-->}{\longrightarrow}
  \SetSymbol{<--}{\longleftarrow}
  \SetSymbol{<-->}{\longleftrightarrow}
  \SetSymbol{|->}{\mapsto}
  \SetSymbol{|-->}{\longmapsto}
  \SetAmsSymbol{>->}{\rightarrowtail}{%
\mathbin{\raise0.18ex\hbox{$\scriptstyle>$}%
\hskip-0.4em-\hskip-0.75em\raise0.18ex\hbox{$\scriptstyle>$}}}
  \SetAmsSymbol{<-<}{\leftarrowtail}{%
\mathbin{\raise0.18ex\hbox{$\scriptstyle<$}%
\hskip-0.75em-\hskip-0.4em\raise0.18ex\hbox{$\scriptstyle<$}}}
  \SetAmsSymbol{->>}{\twoheadrightarrow}{%
\mathbin{-\hskip-0.45em\raise0.28ex\hbox{$\scriptscriptstyle>\hskip-0.3em>$}}}
  \SetAmsSymbol{<<-}{\twoheadleftarrow}{%
\mathbin{\raise0.28ex\hbox{$\scriptscriptstyle<\hskip-0.3em<$}\hskip-0.45em-}}
  \SetSymbol{|-}{\vdash}
  \SetSymbol{|=}{\models\hskip0.1em}
%Boxes and Circles
  \SetAmsSymbol{[]}{\hskip0.1em\square\hskip0.1em}{%
                    \hskip0.1em\copy\@@Box\hskip0.1em}
  \SetAmsSymbol{<>}{\hskip0.1em\lozenge\hskip0.1em}{%
                    \hskip0.1em\Diamond\hskip0.1em}
  \SetSymbol{<|}{\triangleleft}
  \SetSymbol{|>}{\triangleright}
  \SetSymbol{[|]}{\mathbin{\copy\@@BoxBar}}
  \SetAmsSymbol{[-]}{\boxminus}{\mathbin{\copy\@@BoxMinus}}
  \SetAmsSymbol{[+]}{\boxplus}{\mathbin{\copy\@@BoxPlus}}
  \SetAmsSymbol{[*]}{\boxtimes}{\mathbin{\copy\@@BoxTimes}}
  \SetAmsSymbol{[.]}{\boxdot}{\mathbin{\copy\@@BoxDot}}
% Brackets
  \SetSymbol{|[}{\@SemOpen}
  \SetSymbol{]|}{\@SemClose}
  \SetSymbol{||}{\parallel}
% Diverse
  \SetSymbol{/}{\@Slash}   % see above
  \SetSymbol{-;}{\neg\hskip0.1em}
  \SetSymbol{-:-}{\div}
  \SetSymbol{-.}{\mathbin{\@SymDiff}}
  \SetSymbol{:}{\mathpunct{\,:\,}}
  \SetSymbol{...}{\ldots}
  \SetSymbol{"}{\texttt{"}}             % may not work with babel-style
  \SetSymbol{'}{\sp{\prime}}
  \SetSymbol{**}{\times}
  \SetSymbol{><}{\times}
  \SetSymbol{+-}{\pm}
  \SetSymbol{++}{\mathbin{\@Concat}}
  \SetSymbol{::}{\texttt{\upshape:\hskip-0.2em:}}
% using ^ and _
  \SetSymbol{_}{\mathbin{\,\Identifier{\char"5F}\,}}
  \SetSymbol{^}{\Identifier{\char"5E}}
  \SetSymbol{^^}{\Identifier{\char"5E\char"5E}}
  \SetSymbol{|_|}{\sqcup}
  \SetSymbol{_|_}{\bot}
  \SetSymbol{|^}{\uparrow}
% using \
  \SetMacro{\/}{\vee}
%some additional macros
  \SetMacro{\All}{\forall\,}
  \SetMacro{\Ex}{\exists\,}
  \SetMacro{\A}{\forall\,}
  \SetMacro{\E}{\exists\,}
%
  \SetMacro{\U}{\cup}
  \SetMacro{\I}{\cap}
%
  \SetMacro{\o}{\@Compose}
%
  \SetMacro{\\}{\lambda\hskip0.1em}
%
  \SetMacro{\<}{\langle}
  \SetMacro{\>}{\rangle}
%
  \SetAmsSymbol{BB}{\mathbb{B}}{\texttt{I\kern-0.2emB}}
  \SetAmsSymbol{NN}{\mathbb{N}}{\texttt{I\kern-0.2emN}}
  \SetAmsSymbol{RR}{\mathbb{R}}{\texttt{I\kern-0.2emR}}
  \SetAmsSymbol{ZZ}{\mathbb{Z}}{\texttt{Z\kern-0.3emZ}}
  \SetAmsSymbol{QQ}{\mathbb{Q}}{\texttt{I\kern-0.5emQ}}
  \SetAmsSymbol{VV}{\mathbb{V}}{\texttt{V\kern-0.4emV}}
%
}
}

