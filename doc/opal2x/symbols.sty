\def\@SymDiff{\mathbin{%
\hbox to 1em{\hfill$-\kern-.51em\raise.9ex\hbox{.}$\hfill}}}

\newbox\@@Box%
\setbox\@@Box=\hbox{%
\unitlength0.065em%
\begin{picture}(10,10)%
\put(00,00){\line(1,0){10}}%
\put(10,00){\line(0,1){10}}%
\put(10,10){\line(-1,0){10}}%
\put(00,10){\line(0,-1){10}}%
\end{picture}%
}%
%
\def\@Diamond{\hbox{%
\unitlength0.075em%
\begin{picture}(10,10)%
\thicklines%
\put(05,00){\line(1,1){05}}%
\put(10,05){\line(-1,1){05}}%
\put(05,10){\line(-1,-1){05}}%
\put(00,05){\line(1,-1){05}}%
\end{picture}%
}}%
%
\newbox\@@BoxBar%
\setbox\@@BoxBar=\hbox{\hss%
\unitlength0.065em%
\begin{picture}(10,10)%
\put(00,00){\line(1,0){10}}%
\put(10,00){\line(0,1){10}}%
\put(10,10){\line(-1,0){10}}%
\put(00,10){\line(0,-1){10}}%
\put(05,00){\line(0,1){10}}%
\end{picture}%
\hss}%
%
\newbox\@@BoxMinus%
\setbox\@@BoxMinus=\hbox{%
\unitlength0.065em%
\begin{picture}(10,10)%
\put(00,00){\line(1,0){10}}%
\put(10,00){\line(0,1){10}}%
\put(10,10){\line(-1,0){10}}%
\put(00,10){\line(0,-1){10}}%
\put(00,05){\line(1,0){10}}%
\end{picture}%
}%
%
\newbox\@@BoxPlus%
\setbox\@@BoxPlus=\hbox{%
\unitlength0.065em%
\begin{picture}(10,10)%
\put(00,00){\line(1,0){10}}%
\put(10,00){\line(0,1){10}}%
\put(10,10){\line(-1,0){10}}%
\put(00,10){\line(0,-1){10}}%
\put(05,00){\line(0,1){10}}%
\put(00,05){\line(1,0){10}}%
\end{picture}%
}%
%
\newbox\@@BoxTimes%
\setbox\@@BoxTimes=\hbox{%
\unitlength0.065em%
\begin{picture}(10,10)%
\put(00,00){\line(1,0){10}}%
\put(10,00){\line(0,1){10}}%
\put(10,10){\line(-1,0){10}}%
\put(00,10){\line(0,-1){10}}%
\put(05,05){\makebox(0,0){$\times$}}
\end{picture}%
}%
%
\newbox\@@BoxDot%
\setbox\@@BoxDot=\hbox{%
\unitlength0.065em%
\begin{picture}(10,10)%
\put(00,00){\line(1,0){10}}%
\put(10,00){\line(0,1){10}}%
\put(10,10){\line(-1,0){10}}%
\put(00,10){\line(0,-1){10}}%
\put(05,05){\makebox(0,0){.}}%
\end{picture}%
}%
%%
\def\@SemOpen{\hbox to 0.8em{\hfill$[$\kern-.15em$[$\hfill}}

\def\@SemClose{\hbox to 0.8em{\hfill$]$\kern-.15em$]$\hfill}}
%
\def\@Concat{\mathbin{\hbox to 1em{\hfill%
\unitlength0.07em%
\begin{picture}(10,8)%
%\thicklines%
\put(0,4){\line(1,0){3.8}}%
\put(6.2,4){\line(1,0){3.8}}%
\put(3.9,0){\line(0,1){8}}%
\put(6.1,0){\line(0,1){8}}%
\end{picture}%
\hfill}}}%

\def\@SlashBackslash{\wedge}%
\def\@Slash{/}%
%
\def\@Compose{\mathbin{\raisebox{1pt}{$\scriptstyle{\circ}$}}}%
%
\def\@Disjoint{\mathbin{\mbox{$\not$\hspace{0.5pt}$\cap$}}}
%
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% Some special characters
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%
\def\@Ampersand{\char`\&}
\def\@At{\char`\@}
\def\@Backslash{\char`\\}
\def\@Dollar{\char`\$}
\def\@DoubleQuote{\char`\"}     % this is tricky with the babel-style
\def\@Hashmark{\char`\#}
\def\@Percent{\char`\%}


%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% Naming those self-designed symbols
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

\let\Ampersand=\@Ampersand
\let\At=\@At
\def\Backslash{\texttt{\@Backslash}}
\let\Dollar=\@Dollar
\def\DoubleQuote{\texttt{\@DoubleQuote}}
\let\Hashmark=\@Hashmark
\let\Percent=\@Percent

\let\Compose=\@Compose
\let\Disjoint=\@Disjoint

\def\SymDiff{{$\@SymDiff$}}

\def\Box{\copy\@@Box}
\def\Diamond{\@Diamond}
\def\BoxBar{\copy\@@BoxBar}
\def\BoxMinus{\copy\@@BoxMinus}
\def\BoxPlus{\copy\@@BoxPlus}
\def\BoxTimes{\copy\@@BoxTimes}
\def\BoxDot{\copy\@@BoxDot}

\def\SemOpen{\@SemOpen}
\def\SemClose{\@SemClose}

\def\Concat{\hbox{$\@Concat$}}


