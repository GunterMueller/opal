IMPLEMENTATION ChooseOption
-- %- $Header: /home/florenz/opal/home_uebb_CVS/CVS/ocs/src/dosfop/tools/dosfop/mylib/ChooseOption.impl,v 1.1.1.1 1998-06-16 16:00:36 wg Exp $

IMPORT 	Option COMPLETELY
	Seq    COMPLETELY


DEF choose(Fun, Ft :: Rt) ==
    IF avail?(Fun(Ft)) THEN Fun(Ft) ELSE choose(Fun, Rt) FI

DEF choose(Fun, <>) == nil
