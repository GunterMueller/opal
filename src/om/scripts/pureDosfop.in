#!/bin/sh
# $Id$

export OCS=@prefix@/ocs-@PACKAGE_VERSION@

SHELL=/bin/sh
export SHELL

if [ -z "$OCSOM" ]
then
  OCSOM=$OCS/lib/om
  export OCSOM
fi

if [ -z "$OCSPROJECT" ]
then
  OCSPROJECT=$OCSOM/specs/empty
  export OCSPROJECT
fi

if [ -z "$DOSFOP" ]
then
  DOSFOP=$OCS/dosfop
  export DOSFOP
fi

. $OCSOM/specs/ShSpecs.basic

exec $WISH $DOSFOP/tcl/pureDosfop.tcl $*
