       Denotation ONLY ++

DEF `(f1, f2, f3)(data1(d1)) == "[1|" ++ (f1(d1) ++ "]")
DEF `(f1, f2, f3)(data2(d2)) == "[2|" ++ (f2(d2) ++ "]")
DEF `(f1, f2, f3)(data3(d3)) == "[3|" ++ (f3(d3) ++ "]")

