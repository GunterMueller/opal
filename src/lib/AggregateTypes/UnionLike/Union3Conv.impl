IMPORT Union3 COMPLETELY
       Denotation ONLY ++

DEF `(f1, _, _)(data1(d1)) == "[1|" ++ (f1(d1) ++ "]")
DEF `(_, f2, _)(data2(d2)) == "[2|" ++ (f2(d2) ++ "]")
DEF `(_, _, f3)(data3(d3)) == "[3|" ++ (f3(d3) ++ "]")

