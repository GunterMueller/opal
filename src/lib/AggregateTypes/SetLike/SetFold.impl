IMPORT Set ONLY set rep {}?
       Seq ONLY seq
       SeqFold ONLY /
       ABORT ONLY ABORT

DEF + / S == IF S {}? THEN ABORT("/'SetFold: applied to empty set")
             ELSE + / (S rep)
             FI
