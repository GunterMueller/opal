IMPORT ISeq ONLY iseq ft rt ::

DEF merge(<)(s1, s2) == 
	IF ft(s1) < ft(s2) THEN (\\ . ft(s1)) :: (\\ . merge(<)(rt(s1), s2))
	ELSE	(\\ . ft(s2)) :: (\\ . merge(<)(s1, rt(s2)))
	FI
