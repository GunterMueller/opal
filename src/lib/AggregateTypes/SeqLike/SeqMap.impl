IMPORT Seq[from] COMPLETELY
       Seq[to]   COMPLETELY

DEF f*s ==
	IF s<>? THEN <> :seq[to]
		ELSE f(ft(s))::( f*rt(s) ) FI

DEF map(f, s) == f * s
DEF map(f)(s) == f * s