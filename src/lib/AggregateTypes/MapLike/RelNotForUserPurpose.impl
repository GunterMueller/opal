IMPORT CAST[rel, myrel] ONLY cast
       CAST[myrel, rel] ONLY cast

DATA myrel == abs(rep: map[dom, <, map[codom, <, data]])

DEF abs(M) == cast[myrel,rel](abs(M))
DEF rep(R) == rep(cast[rel, myrel](R))