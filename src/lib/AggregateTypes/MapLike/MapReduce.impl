IMPORT Map ONLY map 
       MapNotForUserPurpose ONLY rep
       Pair ONLY pair 1st 2nd unpair
       SetByBST ONLY set
       SetByBSTReduce ONLY /

DEF (o, e)/m == LET oo == \\p,t. 2nd(p) o t
		IN
		   (oo, e)/rep(m)


DEF (o, e)/m == LET oo == \\p,t. (unpair(p)) o t
		IN 
		   (oo, e)/rep(m)

DEF (o : codom ** to -> to / e)(m) == (o, e)/m
DEF (o : dom ** codom ** to -> to / e)(m) == (o, e)/m
