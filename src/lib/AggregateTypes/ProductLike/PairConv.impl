IMPORT Denotation ONLY ++ 
       Pair ONLY pair &


DEF `(f1, f2)(p) == `("(", ",", ")")(f1,f2)(p)

DEF `(lpar,sep,rpar)(f1,f2)(d1&d2) == 
    lpar ++ (f1(d1) ++ (sep ++ (f2(d2) ++ rpar)))
