IMPORT Denotation ONLY ++
       Triple ONLY triple &

DEF `(f1, f2, f3)(t) == `("(", ",", ")")(f1,f2,f3)(t)

DEF `(lpar,sep,rpar)(f1,f2,f3)(&(d1,d2,d3)) ==
    lpar ++ (f1(d1) ++ (sep ++ (f2(d2) ++ (sep ++ (f3(d3) ++ rpar)))))
		
