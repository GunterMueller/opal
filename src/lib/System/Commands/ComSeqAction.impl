IMPORT	Seq		COMPLETELY
	Com		COMPLETELY
	ComCompose	COMPLETELY
	ComAction 	COMPLETELY


DEF C @; (X :: Xs) ==
    C(X) ; C @; Xs

DEF _ @; <> ==
    done

DEF C @& (X :: Xs) ==
    C(X) & C @& Xs

DEF _ @& <> ==
    done
