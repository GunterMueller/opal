-- function composition

SORT arg inter res

FUN o : (inter -> res) ** (arg -> inter) -> (arg -> res) 
-- (f o g)(x) == f(g(x))

FUN ; : (arg -> inter) ** (inter -> res) -> (arg -> res) 
-- (g ; f)(x) == f(g(x))




