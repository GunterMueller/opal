IMPORT Nat ONLY nat < > <= >= = |=
          
DEF a < b  ==  inj(a) <  inj(b)
DEF a > b  ==  inj(a) >  inj(b)
DEF a <= b ==  inj(a) <= inj(b)
DEF a >= b ==  inj(a) >= inj(b)
DEF a = b  ==  inj(a) =  inj(b)
DEF a |= b ==  inj(a) |= inj(b)
