/* %Generate a formatting function */

SIGNATURE ordFmtPrint

IMPORT InterOpal ONLY interOpal:SORT name:SORT object:SORT variant:SORT
       Seq[variant] ONLY seq

-- %@code{mkFmtPrint(IO, sort, variants) == (newIo, fmtPrint)}
FUN mkFmtPrint: interOpal ** name ** seq[variant] -> interOpal ** object
