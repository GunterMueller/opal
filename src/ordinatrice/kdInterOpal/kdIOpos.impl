IMPLEMENTATION kdIOpos
-- $Header: /home/florenz/opal/home_uebb_CVS/CVS/ocs/src/ordinatrice/kdInterOpal/kdIOpos.impl,v 1.1.1.1 1998-06-16 16:00:46 wg Exp $
--

IMPORT InterOpal COMPLETELY
	kdIOpart COMPLETELY
	NatConv ONLY `
	Nat ONLY nat < = <=
	Denotation ONLY ++ 

DEF unknown` == "pos:unknown"
DEF part(p)` == "pos:" ++ (p`)
DEF pos(p,r,c)` == "pos:" ++ (p`) ++ ":" ++ (r`) ++ "," ++ (c`)


DEF pos(_,r1,c1) < pos(_,r2,c2) == 
	r1 < r2 or (r1 = r2 and c1 < c2)

DEF x:pos < y:pos == false
