/* %compute a sequence of expressions for a whitebox test */

SIGNATURE WhiteBox

IMPORT Repo ONLY repo:SORT
       RepoName ONLY objectName:SORT
       Pair[seq[expr], diag] ONLY pair
       Seq[expr] ONLY seq
       RepoAbsy ONLY expr:SORT
       RepoEvent ONLY diag:SORT


FUN whiteBox: repo ** objectName -> pair[seq[expr], diag]