SIGNATURE ProofCorrectness

IMPORT RepoName ONLY unitName:SORT
       Repo ONLY repo:SORT
       Com[pair] ONLY com:SORT
       Pair[diag, repo] ONLY pair
       RepoDiag ONLY diag:SORT
       Real ONLY real:SORT

/* %Check proofs in unit with respect to repository, which have the
    denotation as prefix. */

FUN proofCorrectness: unitName -> repo -> denotation ** real ->
                       com[pair[diag, repo]]

