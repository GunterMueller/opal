/* %
Generic pretty-printing via abstract data type (linear time).
*/
IMPLEMENTATION NFmt
-- $Id$


IMPORT
  Nat ONLY nat 
  Seq ONLY seq
  Fmt COMPLETELY

DEF fmtLatin1 : fmt ** nat ** fmt -> seq[string] == pretty
DEF fmtLatin1 : nat ** nat ** fmt -> seq[string] == pretty
DEF fmtLatin1 : fmt ** nat ** fmt -> string == pretty
DEF fmtLatin1 : nat ** nat ** fmt -> string == pretty
DEF fmtLatin1 : fmt               -> string == pretty
DEF fmtLatin1 : fmt               -> denotation == pretty


