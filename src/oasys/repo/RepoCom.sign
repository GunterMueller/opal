SIGNATURE RepoCom

IMPORT	Void		ONLY	void
	Com[void]	ONLY	com
	Repo		ONLY	repo:SORT
	Com[repo]	ONLY	com

FUN upd : (repo -> repo) -> com[void]
FUN upd : (repo -> com[repo]) -> com[void]


-- not for user purpose ============
FUN setRepository : repo -> com[void]
FUN getRepository : com[repo]
FUN getAndClearRepository : com[repo]
