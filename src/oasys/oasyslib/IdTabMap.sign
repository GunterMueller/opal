/* % This structure provides mappings on @code{idtab'IdTab}. */

SIGNATURE IdTabMap[codom1, codom2]

SORT codom1 codom2

IMPORT  Id              ONLY id:SORT
        IdTab[codom1]   ONLY idtab:SORT
        IdTab[codom2]   ONLY idtab:SORT

FUN *   : (codom1 -> codom2) ** idtab[codom1] -> idtab[codom2]
