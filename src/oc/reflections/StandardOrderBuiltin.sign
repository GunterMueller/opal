SIGNATURE StandardOrderBuiltin

IMPORT Map ONLY map
       Method[sto, bool] ONLY method : SORT
       Nat ONLY nat
       Seq ONLY seq
       Set ONLY set
       Reflection ONLY type : SORT value : SORT

TYPE sto == =(x : value, y : value)
	    <(x : value, y : value)

FUN nat : sto -> bool

FUN map : method -> method
FUN seq : method -> method
FUN set : method -> method

FUN default : method

